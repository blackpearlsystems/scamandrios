{
    "name": "scamandrios",
    "version": "0.7.0",
    "description": "node.js bindings for Cassandra with a promises API",
    "keywords":
    [
        "cassandra",
        "thrift"
    ],
    "contributors":
    [
        "Kit Cambridge <kitcambridge@me.com>",
        "C J Silverio <ceejceej@gemail.com>",
        "Russell Bradberry <rbradberry@gmail.com>",
        "Christoph Tavan <dev@tavan.de>",
        "Calvin French-Owen <calvin@segment.io>"
    ],
    "homepage": "https://github.com/ceejbot/scamandrios",
    "repository":
    {
        "url": "git://github.com/ceejbot/scamandrios.git"
    },
    "dependencies":
    {
        "helenus-thrift": "0.7.3",
        "node-uuid": "*",
        "p-promise": "*",
        "lodash": "*"
    },
    "devDependencies":
    {
        "whiskey": "git://github.com/cloudkick/whiskey.git#b3c5bc23e30c95e46083bc7628c2557c1c15ec95",
        "JSDoc": "git://github.com/jsdoc3/jsdoc.git",
        "mocha-as-promised": "*",
        "chai-as-promised": "*",
        "travis-cov": "*",
        "blanket": "*"
    },
    "scripts":
    {
        "test": "make test",
        "blanket":
        {
            "pattern": "//^((?!\/node_modules|test|doc|examples\/).)*$/ig",
            "onlyCwd": true
        },
        "travis-cov": { "threshold": 50 }
    },
    "main": "index",
    "engines": { "node": ">= 0.10" },
    "license": "MIT",
    "readmeFilename": "README.md"
